{
	"schemaVersion": 1,
	"id": "preventthespread",
	"version": "${version}",
	"name": "Prevent the Spread",
	"description": "Locate, identify, and treat corrupted clusters.",
	"authors": [
		"opl"
	],
	"contact": {
		"homepage": "https://carpercreative.com/"
	},
	"license": "UNLICENSED",
	"icon": "assets/preventthespread/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.carpercreative.preventthespread.PreventTheSpread",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.carpercreative.preventthespread.client.PreventTheSpreadClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.carpercreative.preventthespread.datagen.PreventTheSpreadDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"preventthespread.mixins.json",
		{
			"config": "preventthespread.client.mixins.json",
			"environment": "client"
		}
	],
	"accessWidener": "preventthespread.accesswidener",
	"depends": {
		"fabricloader": ">=0.15.10",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.23"
	}
}
